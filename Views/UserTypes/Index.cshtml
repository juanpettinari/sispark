@model IEnumerable<SisPark.Models.Usertype>

@{
    ViewBag.Title = "Tipos de usuario";
}

<h2>Tipos de Usuario</h2>

<p>
    @Html.ActionLink("Nuevo Tipo de usuario", "Create")
</p>
<table>
    <tr>
        <th>
            Tipo
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.description)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.usertypeid })
        </td>
    </tr>
}

</table>
