@model SisPark.Models.Ticket

@{
    ViewBag.Title = "TicketCancelado";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>TicketCancelado</h2>


<h3> Estás seguro que querés cancelar este ticket?</h3>

<fieldset>
    <div class="display-label">
        @Html.DisplayNameFor(model => model.Car.Patente)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Car.Patente)
    </div>

    <div class="display-label">
        @Html.DisplayNameFor(model => model.TicketState.description)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.TicketState.description)
    </div>

    <div class="display-label">
        @Html.DisplayNameFor(model => model.StartDateTime)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.StartDateTime)
    </div>

</fieldset>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Enviar" /> |
        @Html.ActionLink("Volver a la lista", "RetirarCoche", "Tickets")
    </p>
}