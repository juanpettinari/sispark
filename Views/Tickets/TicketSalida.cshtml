@model SisPark.Models.Ticket

@{
    ViewBag.Title = "TicketSalida";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>TicketSalida</h2>

<h3> Estás seguro que querés retirar este auto?</h3>

<fieldset>
    <div class="display-label">
        @Html.DisplayNameFor(model => model.Car.Patente)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Car.Patente)
    </div>
    <div class="display-label">
        @Html.DisplayNameFor(model => model.Car.NombreCliente)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Car.NombreCliente)
    </div>
    <div class="display-label">
        @Html.DisplayNameFor(model => model.StartDateTime)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.StartDateTime)
    </div>

</fieldset>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Retirar" /> |
        @Html.ActionLink("Volver a la lista", "RetirarCoche","Tickets")
    </p>
}